{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "you-get-ui",
  "version": "0.1.0",
  "identifier": "com.tudan.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "you-get-ui",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "open": true,
      "execute": true
    },
    "fs": {
      "scope": ["**"],
      "allow": ["read", "write"]
    }
  }
}
